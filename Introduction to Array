//https://www.hackerrank.com/challenges/arrays-introduction/problem

#include<iostream>
#include<string>
#include<cstdio>
#include<stdio.h>
#include<math.h>
#include<cstdlib>
using namespace std;

int main()
{
    int n,i;
    cin>>n;
    int box[n];
    for(i=0;i<n;i++){
        cin>>box[i];
    }
    for(i=n-1;i>=0;i--){
       cout<<box[i]<<" ";
    }

    return 0;
}



